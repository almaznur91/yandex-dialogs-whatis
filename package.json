{
  "name": "yandex-dialogs-whatis",
  "version": "0.0.1",
  "description": "Позволяет добавлять свои вопросы и потом получать на них ответы",
  "files": [
    "src"
  ],
  "main": "lambda.js",
  "scripts": {
    "start": "node src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "claudia": "claudia",
    "generate": "npm run claudia -- generate-serverless-express-proxy --express-module src/lambda",
    "create": "npm run claudia -- create --name yandex-dialogs-whatis --region eu-west-1 --handler lambda.handler --set-env DB_DRIVER=mongo --profile yandex-dialogs-whatis",
    "update": "npm run claudia -- update --profile yandex-dialogs-whatis"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/popstas/yandex-dialogs-whatis.git"
  },
  "author": "Stanislav Popov <popstas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/popstas/yandex-dialogs-whatis/issues"
  },
  "homepage": "https://github.com/popstas/yandex-dialogs-whatis#readme",
  "dependencies": {
    "aws-serverless-express": "^3.2.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "fuse.js": "^3.2.1",
    "lokijs": "^1.5.5",
    "mongodb": "^3.1.0",
    "yandex-dialogs-sdk": "github:popstas/yandex-dialogs-sdk"
  },
  "devDependencies": {
    "claudia": "^5.0.0"
  }
}
